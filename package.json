{
  "name": "eduverse",
  "version": "1.0.0",
  "description": "Web3 Educational Platform on Manta Network",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "test": "hardhat test",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "deploy:separated:local": "hardhat run scripts/deploy-separated.js --network localhost",
    "deploy:manta": "hardhat run scripts/deploy.js --network mantaPacificTestnet",
    "deploy:separated:manta": "hardhat run scripts/deploy-separated.js --network mantaPacificTestnet",
    "verify:manta": "hardhat run scripts/verify-separated.js --network mantaPacificTestnet",
    "console": "hardhat console",
    "interact:testnet": "hardhat run scripts/testnet-interact.js --network mantaPacificTestnet",
    "interact:update": "hardhat run scripts/update_course.js --network mantaPacificTestnet",
    "explore-courses": "hardhat run scripts/testing-explore-courses.js --network mantaPacificTestnet",
    "my-licenses": "hardhat run scripts/testing-my-licenses.js --network mantaPacificTestnet",
    "export-abis": "hardhat compile && node scripts/ABI-Export.js",
    "update-mobile-env": "node scripts/update-mobile-env.js",
    "setup-mobile": "npm run export-abis && npm run update-mobile-env",
    "android": "expo run:android",
    "ios": "expo run:ios"
